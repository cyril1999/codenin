package MultidimensionalArrays;

import java.util.Scanner;

public class LargestRoworColumn {

    public static int[][] takeinput() 
	{
		
	Scanner sc = new Scanner(System.in);
		
		int rows = sc.nextInt();
		int cols = sc.nextInt();
		int[][] arr = new int [rows][cols];
		//Input of array
		for (int i = 0; i < rows; i++) {

			for (int j = 0; j < cols; j++) {
				
				arr[i][j] = sc.nextInt();
			}
		}
		return arr;
	}
	
    public static void output(int[][] arr2d) //reference value inside input passed to arr2d
	{
		
	Scanner sc = new Scanner(System.in);
		
	
		int rows = arr2d.length;
		int cols = arr2d[0].length;
		int rowsum = 0;
		int columnsum = 0;
		int largestRow = Integer.MIN_VALUE;
		int largestRowIndex = 0;
		int largestColumn = Integer.MIN_VALUE;
		int largestColumnIndex = 0;
		
		//Output of array
		for (int i = 0; i < rows ; i++) {
			rowsum = 0;
			
			for (int j = 0; j < cols ;j++) 
			{
			rowsum+= arr2d[i][j];
				
			}
			if (rowsum >largestRow) 
			{
			largestRow = rowsum;
			largestRowIndex = i;
			}
			
		}
		for (int i = 0; i < cols ; i++) {
			rowsum = 0;
			
			for (int j = 0; j < rows ;j++) 
			{
			columnsum+= arr2d[j][i];
				
			}
			if (columnsum >largestColumn) 
			{
			largestColumn = columnsum;
			largestColumn = i;
			}
		}
		if (largestRow>=largestColumn) 
		{
			System.out.println("row" + " " + largestRowIndex + " " + largestRow);
		} else {
			System.out.println("column" + " " + largestColumnIndex + " " + largestColumn);
		}
		
	}
    
    
    public static void main(String[] args) 
    {
		int [][] input = takeinput();
		output(input);       

	}
}
